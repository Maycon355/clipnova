    "@vercel/kv": "^3.0.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "bull": "^4.11.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ioredis": "^5.3.2",
    "next": "14.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "tailwindcss": "^3.4.1",
